{
  "name": "squadeasy-api",
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "build-tsp-json": "tsp compile . --options=@typespec/openapi3.file-type=json",
    "build-tsp-yaml": "tsp compile .",
    "build": "concurrently \"npm:build-tsp-*\"",
    "compile-zod-client": "openapi-zod-client ./tsp-output/schema/openapi.yaml",
    "serve": "scalar serve tsp-output/schema/openapi.json",
    "test": "vitest"
  },
  "private": true,
  "devDependencies": {
    "@typespec/compiler": "latest",
    "@typespec/http": "latest",
    "@typespec/rest": "latest",
    "@typespec/openapi": "latest",
    "@typespec/openapi3": "latest",
    "@scalar/cli": "^0.2.97",
    "concurrently": "^8.2.2"
  }
}
